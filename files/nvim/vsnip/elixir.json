{
  "inspect with label": {
    "prefix": "in",
    "body": "IO.inspect($1, label: \"$1\")"
  },
  "pipe inspect with label": {
    "prefix": "i",
    "body": "|> IO.inspect(label: \"$1\")"
  },
  "inspect selected": {
    "prefix": "is",
    "body": "IO.inspect(${1:$TM_SELECTED_TEXT})"
  },
  "do": {
    "prefix": "do",
    "body": ["do", "  $0", "end"]
  },
  "IO.puts": {
    "prefix": "p",
    "body": "IO.puts($0)"
  },
  "puts with label": {
    "prefix": "pu",
    "body": "IO.puts(\"$1: #{inspect($1)}\")"
  }
}
